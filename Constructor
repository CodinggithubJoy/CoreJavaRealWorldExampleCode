// how to create a constructor and implement 

package CoreJavawithCode;

class Cons{
	String name;
	int age;
	
	   Cons() {
		 name = "joy";
		 age = 23;
	}
	public void display() {
		System.out.println("Name is :" + name);
		System.out.println("Age is :" + age);
	}
}

public class Constructor {

	public static void main(String[] args) {
		Cons c = new Cons();
		c.display();
	}

}
