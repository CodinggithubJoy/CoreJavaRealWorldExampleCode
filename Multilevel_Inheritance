// Multilevel Inheritance

package CoreJavawithCode;

class Library{
	public void m1() {
		System.out.println(" you can access any kind of book from library");
	}
}

class Book extends Library {
	public void m2() {
		System.out.println("First You can access core java book");
	}
}

class Java extends Book{
	public void m3() {
		System.out.println("After Core java Completed you can access Advanced java");
	}
}

class Advanced extends Java{
	public void m4() {
		System.out.println("Then if you learn Corejava after Advanced java you are Backend developer");
	}
}

public class MultiLevel_Inheritance {

	public static void main(String[] args) {
		Advanced j = new Advanced();
		j.m1();
		j.m2();
		j.m3();
		j.m4();
	}

}
